{
  "name": "rss_aggregator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && swc ./src -d dist",
    "db:console": "docker compose run postgres psql -h postgres -U postgres -d rss_aggregator",
    "start": "node dist/src/index.js",
    "db:migrate": "prisma migrate dev",
    "rebuild:be": "docker compose build backend && docker compose rm --force --stop backend && docker compose up -d backend",
    "seed:dev": "docker compose run backend pnpm ts-node scripts/seed.ts",
    "start:prod": "node --inspect=0.0.0.0 dist/src/index.js",
    "start:docker": "pnpm build && pnpm start:prod",
    "watch": "watchy --no-init-spawn --debounce 1 -w src -- docker compose restart backend"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "volta": {
    "node": "20.11.0"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "rss-parser": "^3.13.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.3.9",
    "@swc/core": "^1.4.0",
    "@tsconfig/node20": "^20.1.2",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "watchy": "^0.10.1"
  }
}
